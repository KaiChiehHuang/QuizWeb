<?xml version="1.0"?>
<quiz random="true" one-page="false" practice-mode="false" immediate-correction="true">
<title>Geography Exam</title>
<quiz-image>https://pioneerwoman.files.wordpress.com/2013/04/geographyquiz2.jpg</quiz-image>
<category>Geography</category>
<description>Let's test how well you know about geography.</description>

<question type="picture-response">
  <query>What continent is in this picture?</query>
  <image-location>http://www.ethnologue.com/sites/default/files/maps/afr_web_1.png</image-location>
  <answer-list>
    <answer preferred="preferred">Africa</answer>
    <answer>feizhou</answer>
  </answer-list>
</question>

<question type="picture-response">
  <query>Which country is in this picture?</query>
  <image-location>https://recodetech.files.wordpress.com/2014/05/china-1-of-1.jpg?quality=80&strip=info&w=640</image-location>
  <answer>China</answer>
  <answer>Chinese</answer>
  <answer>P.R.C</answer>
</question>

<question type="fill-in-blank">
  <blank-query><pre></pre><blank /><post>is the biggest country in the world.</post></blank-query>
  <answer>Russia</answer>
  <answer>The Russia</answer>
</question>

<question type="multiple-choice">
  <query>California, New Mexico and Canada are all_____________</query>
  <option answer="answer">Bigger than Stanford</option>
  <option>Countries</option>
  <option>States</option>
  <option answer="answer">In North America</option>
</question>

<question type="multiple-response" answer-ordered="true">
  <query>List the top three biggest oceans in the world.</query>
  <answer-list>
    <answer>Pacific</answer>
  </answer-list>
  <answer-list>
    <answer>Atlantic</answer>
  </answer-list>
  <answer-list>
    <answer>Indian</answer>
  </answer-list>
</question>

</quiz>